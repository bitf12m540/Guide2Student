         </div> 

    </nav>


   

    <div style="width:20%;float:left">

        <div class="row">
            <div class="col-md-9">
                <div class="list-group">
                    <a class="list-group-item active"> Provinces</a>
                    <a href="/Home/SearchProvince/?Province=Punjab" class="list-group-item">Punjab</a>
                    <a href="/Home/SearchProvince/?Province=Sindh" class="list-group-item">Sindh</a>
                    <a href="/Home/SearchProvince/?Province=Balochistan" class="list-group-item">Balochistan</a>
                    <a href="/Home/SearchProvince/?Province=KPK" class="list-group-item">KPK</a>
                    <a href="/Home/SearchProvince/?Province=All" class="list-group-item">All</a>

                </div>
            </div>
           
        </div>
       
       
    </div>
    <div style="width:80%;float:right">
        @RenderSection("Index", required: false)
        @RenderSection("CalculateMerit", required: false)
      
      
        @RenderSection("ContactUs", required: false)
        @RenderSection("SearchProvince",required:false)
        @RenderSection("UnivDetail",required:false)
        @RenderSection("SuggestedUniv", required: false)
        @RenderSection("Search", required: false)
        @RenderSection("SearchDegree", required: false)
        @RenderSection("Message", required: false)
        @RenderSection("SendMessage", required: false)
    </div>


    @RenderBody()


   
   
   


   

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
