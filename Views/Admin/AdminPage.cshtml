@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AdminPage</title>
    <script src="~/Scripts/jquery.tablesorter.min.js"></script>
    <script src="~/Scripts/jquery.tablesorter.pager.js"></script>
    <script src="~/Scripts/jquery.wysiwyg.js"></script>
    <script src="~/Scripts/jquery-1.3.2.min.js"></script>
    <script src="~/Scripts/thickbox.js"></script>
    <script src="~/Scripts/jquery.pstrength-min.1.2.js"></script>

    <link href="~/Content/themes/base/minified/grid.css" rel="stylesheet" />
    <link href="~/Content/themes/base/minified/ie.css" rel="stylesheet" />
    <link href="~/Content/themes/base/minified/ie6.css" rel="stylesheet" />
    <link href="~/Content/themes/base/minified/jquery.wysiwyg.css" rel="stylesheet" />
    <link href="~/Content/themes/base/minified/thickbox.css" rel="stylesheet" />
    <link href="~/Content/themes/base/minified/reset.css" rel="stylesheet" />
    <link href="~/Content/themes/base/minified/theme-blue.css" rel="stylesheet" />
    <link href="~/Content/themes/base/minified/styles.css" rel="stylesheet" />
    <link href="~/Content/themes/base/minified/tablesorter.css" rel="stylesheet" />


    <script type="text/javascript">
        $(function () {
            $('#wysiwyg').wysiwyg(
			{
			    controls: {
			        separator01: { visible: true },
			        separator03: { visible: true },
			        separator04: { visible: true },
			        separator00: { visible: true },
			        separator07: { visible: false },
			        separator02: { visible: false },
			        separator08: { visible: false },
			        insertOrderedList: { visible: true },
			        insertUnorderedList: { visible: true },
			        undo: { visible: true },
			        redo: { visible: true },
			        justifyLeft: { visible: true },
			        justifyCenter: { visible: true },
			        justifyRight: { visible: true },
			        justifyFull: { visible: true },
			        subscript: { visible: true },
			        superscript: { visible: true },
			        underline: { visible: true },
			        increaseFontSize: { visible: false },
			        decreaseFontSize: { visible: false }
			    }
			});
        });
    </script>

    <!-- Initiate tablesorter script -->
    <script type="text/javascript">
        $(document).ready(function () {
            $("#myTable")
            .tablesorter({
                // zebra coloring
                widgets: ['zebra'],
                // pass the headers argument and assing a object
                headers: {
                    // assign the sixth column (we start counting zero)
                    6: {
                        // disable it by setting the property sorter to false
                        sorter: false
                    }
                }
            })
        .tablesorterPager({ container: $("#pager") });
        });
    </script>
    <!-- Initiate password strength script -->
   

</head>
<body>
    
    <div id="header">
        <!-- Header. Status part -->
        <div id="header-status">
            <div class="container_12">
                <div class="grid_8">
                    &nbsp;
                </div>
                
                <div class="grid_4">
                    <a href="~/Admin/AdminPage" id="AdminHome">
                        Home
                    </a>
                    <a href="~/Home/Index" id="logout">
                        Logout
                    </a>
                </div>
            </div>
            <div style="clear:both;"></div>
        </div> <!-- End #header-status -->
        <!-- Header. Main part -->
       
        <div style="clear: both;"></div>
        <!-- Sub navigation -->
       
    </div> <!-- End #header -->
    <div class="container_12">

        <!-- Dashboard icons -->
        <div class="grid_7">
            <a href="/Admin/RegiteredUniversities" class="dashboard-module">

                <img src="~/Content/themes/base/images/Crystal_Clear_write.gif" width="64" height="64" alt="edit"/>
                <span>Registered Universities</span>
            </a>

            <a href="~/Admin/UserDetails" class="dashboard-module">

                <img src="~/Content/themes/base/images/Crystal_Clear_write.gif" width="64" height="64" alt="edit" />
                <span>Users Details</span>
            </a>

            
            <a href="~/Admin/Messages" class="dashboard-module">
                <img src="~/Content/themes/base/images/message.png" width="64" height="64" alt="messages" />
                <span>Messages</span>
                <span> 2 Messages</span>
            </a>

            

            

            

         <!--   <a href="" class="dashboard-module">

                <img src="~/Content/themes/base/images/Crystal_Clear_settings.gif" width="64" height="64" alt="edit" />
                <span>Settings</span>
            </a>-->
            <div style="clear: both"></div>
        </div> <!-- End .grid_7 -->

        <div style="clear:both;"></div>



        <div class="grid_12">

           


            <!-- Example table -->
            <div class="module">
                <h2><span>New Notifications </span></h2>

                <div class="module-table-body">
                    <form action="">
                        <table id="myTable" class="tablesorter">
                            <thead>
                                <tr>
                                    <th style="width:5%">#</th>
                                    <th style="width:20%">Institute Name</th>
                                    <th style="width:21%">Degrees Offered</th>
                                    <th style="width:13%">Province</th>
                                    <th style="width:13%">City</th>
                                    <th style="width:13%">Public/Private</th>
                                    <th style="width:15%"></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="align-center">1</td>
                                    <td><a href="">Punjab University</a></td>
                                    <td>Chemical Engineering,Information Technology,LLB</td>
                                    <td>Punjab</td>
                                    <td>Lahore</td>
                                    <td>Public</td>
                                    <td>
                                        
                                        <a href="#">
                                            <img src="~/Content/themes/base/images/tick-circle.gif" width="16" height="16" alt="published" />
                                        </a>

                                        <a href="#">
                                            <img src="~/Content/themes/base/images/Button-Close-icon.png" width="16" height="16" alt="delete" />
                                        </a>
                                    </td>
                                </tr>

                            </tbody>
                        </table>
                    </form>

                    
                    <div style="clear: both"></div>
                </div> <!-- End .module-table-body -->
            </div> <!-- End .module -->

        </div> <!-- End .grid_12 -->

</div>

</body>
</html>
